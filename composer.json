{
  "name": "weemen/blogpost-bundle",
  "require": {
    "symfony/symfony": "2.7.*",
    "weemen/blogpost-domain": "dev-master",
    "weemen/elasticsearch-repository": "dev-master",
    "friendsofsymfony/rest-bundle": "~1.5",
    "jms/serializer-bundle": "~0.13",
    "elasticsearch/elasticsearch": "~1.0",
    "simgroep/event-sourcing": "dev-master",
    "jurjean/spray-serializer-bundle": "dev-master",
    "jurjean/spray-serializer": "dev-master",
    "zendframework/zend-crypt": "^2.5",
    "react/react": "0.4.*",
    "bunny/bunny": "0.1.*",
    "abraham/twitteroauth": "^0.6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "4.7.*"
  },
  "autoload": {
    "psr-4": {
      "Weemen\\BlogPostBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Weemen\\BlogPostBundle\\": "test"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/weemen/blogpost-domain"
    },
    {
      "type": "vcs",
      "url": "https://github.com/simgroep/event-sourcing.git"
    },
    {
      "type": "vcs",
      "url": "https://github.com/weemen/elasticsearch-repository.git"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "2.2.x-dev"
    }
  }
}
